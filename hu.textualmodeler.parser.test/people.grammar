grammar people <People>;
import basics;

terminal MAN "man";
terminal WOMAN "woman";

<People> :- people=<Human>*;

<Human> :- <T> <Content> };

<T> :- MAN {"http://people/1.0#Man";
<T> :- WOMAN {"http://people/1.0#Woman";

<Content> :- name=IDENTIFIER father=IDENTIFIER?[../people/=name];

