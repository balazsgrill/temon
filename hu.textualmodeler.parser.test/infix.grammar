grammar infix <E>;
import basics;

terminal PLUS "\+";
terminal MINUS "\-";

terminal MULTI "\*";
terminal DIVIDE "\/";

<E> :- <E1>;
<E1> :- <E2>;
<E1> :- <E2> PLUS <E1>;
<E1> :- <E2> MINUS <E1>;

<E2> :- <E3>;
<E2> :- <E3> MULTI <E2>;
<E2> :- <E3> DIVIDE <E2>;

<E3> :- QUALIFIEDID;
<E3> :- DECIMAL_NUMBER;