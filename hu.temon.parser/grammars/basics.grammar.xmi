<?xml version="1.0" encoding="ASCII"?>
<grammar:GrammarModel
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:grammar="http://temon.hu/grammar"
    xmi:id="_3yl34LGQEeK1ScU5DNcoWg"
    startItem=""
    name="basics">
  <terminals
      xmi:id="_gP1EELGDEeKaMu577qmfxA"
      name="WHITESPACE"
      regex="\s+"
      hide="true"/>
  <terminals
      xmi:id="_tb_jYLGDEeKaMu577qmfxA"
      name="OPERATOR_SEMICOLON"
      regex=";"
      priority="10"/>
  <terminals
      xmi:id="_qOGHMLGHEeK-59W38_3F4g"
      name="IDENTIFIER"
      regex="[a-zA-Z]\w*"/>
  <terminals
      xmi:id="_mVyX4LGSEeK1ScU5DNcoWg"
      name="WHITESPACE_MLCOMMENT"
      regex="/\*([^*]|[\r\n]|(\*+([^*/]|[\r\n])))*\*+/"
      hide="true"/>
  <terminals
      xmi:id="_yyqIQLGSEeK1ScU5DNcoWg"
      name="WHITESPACE_SLCOMMENT"
      regex="//.*"
      hide="true"/>
  <terminals
      xmi:id="_0qbswLGTEeK1ScU5DNcoWg"
      name="DECIMAL_NUMBER"
      regex="[0-9]+"
      priority="5"/>
  <terminals
      xmi:id="_4LRWQLGTEeK1ScU5DNcoWg"
      name="BINARY_NUMBER"
      regex="0b(0|1)+"
      priority="5"/>
  <terminals
      xmi:id="__7g_cLGTEeK1ScU5DNcoWg"
      name="HEXADECIMAL_NUMBER"
      regex="0x[a-fA-F0-9]+"
      priority="5"/>
  <terminals
      xmi:id="_uhiJ8LGVEeK1ScU5DNcoWg"
      name="STRING"
      regex="([&quot;'])(?:\\\1|.)*?\1">
    <convertFrom
        search="\&quot;"/>
    <convertTo
        search="^"
        replace="\&quot;"/>
    <convertTo
        search="$"
        replace="\&quot;"/>
  </terminals>
  <terminals
      name="OPERATOR_DOT"
      regex="\."/>
  <rules nonTerminal="QUALIFIEDID">
    <body xsi:type="grammar:TerminalItem"
        terminal="_qOGHMLGHEeK-59W38_3F4g"/>
    <body xsi:type="grammar:NonTerminalItem"
        optional="true"
        many="true"
        nonTerminal="QUALIFIEDID_tail"/>
  </rules>
  <rules nonTerminal="QUALIFIEDID_tail">
    <body xsi:type="grammar:TerminalItem"
        terminal="//@terminals.9"/>
    <body xsi:type="grammar:TerminalItem"
        terminal="_qOGHMLGHEeK-59W38_3F4g"/>
  </rules>
</grammar:GrammarModel>
